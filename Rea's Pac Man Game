var instructions = createSprite(220,200);
instructions.setAnimation("instructions");
instructions.scale = 0.5;

var youwin = createSprite(200,200);
youwin.setAnimation("youwin");
youwin.scale = 0.83;

var gameover2 = createSprite(200,200);
gameover2.setAnimation("gameover2");
gameover2.scale = 2;

var gameover = createSprite(200,200);
gameover.setAnimation("gameover");

var tryagain = createSprite(190,350);
tryagain.setAnimation("tryagain");

function draw() {
  background("black");
  wall.setColorEach("blue");
  
  fill("white");
  textSize(19);
  text("Score: " + score, 170, 5, 100, 100);
  
  instructions.visible = false;
  youwin.visible = false;
  gameover2.visible = false;
  gameover.visible = false;
  tryagain.visible = false;
  
  if(mousePressedOver(howtoplay)){
    instructions.visible = true;
  }
  
  if(score == 205){
    youwin.visible = true;
  }
  
  if(pac.isTouching(redghost)){
    gameover2.visible = true;
    gameover.visible = true;
    tryagain.visible = true;
  }
  
  if(mousePressedOver(tryagain)){
    pac.x = 35;
    pac.y = 370;
    redghost.x = 205;
    redghost.y = 210;
    gameover2.visible = false;
    gameover.visible = false;
    tryagain.visible = false;
  }
  
  pac.collide(wall);
  redghost.collide(wall);
  
  if(keyDown("right")) {
    pac.x = pac.x + 3;
    pac.setAnimation("pacman");
  }
  
  if(keyDown("left")){
    pac.x = pac.x - 3;
    pac.setAnimation("pacman_left");
  }
  
  if(keyDown("up")){
    pac.y = pac.y - 3;
    pac.setAnimation("pacman_up");
  }
  
  if(keyDown("down")){
    pac.y = pac.y + 3;
    pac.setAnimation("pacman_down");
  }
  
  if(keyDown("A")){
    redghost.x = redghost.x - 3;
    redghost.setAnimation("redghost_left");
  }
  
  if(keyDown("D")){
    redghost.x = redghost.x + 3;
    redghost.setAnimation("redghost");
  }
  
  if(keyDown("W")){
    redghost.y = redghost.y - 3;
    redghost.setAnimation("redghost_up");
  }
  
  if(keyDown("S")){
    redghost.y = redghost.y + 3;
    redghost.setAnimation("redghost_down");
  }
  
  if(pac.x > 400){
    pac.x = 0;
  }
  
  if(pac.x < 0){
    pac.x = 400;
  }
  
  if(redghost.x > 400){
    redghost.x = 0;
  }
 
  if(redghost.x < 0){
    redghost.x = 400;
  }
  
  if(pac.isTouching(pellet1)){
